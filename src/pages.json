			},
			{
				"pagePath": "pages/classification/list",
				"iconPath": "static/tab-cart.png",
				"selectedIconPath": "static/tab-cart-current.png",
				"text": "分类"
			},
			{
				"pagePath": "pages/index/user",
				"iconPath": "static/tab-my.png",
				"selectedIconPath": "static/tab-my-current.png",
				"text": "我的"
			}
		]
	}
